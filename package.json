{
  "name": "canvas",
  "description": "Canvas graphics API backed by Cairo",
  "version": "3.0.0",
  "author": "TJ Holowaychuk <tj@learnboost.com>",
  "main": "index.js",
  "browser": "browser.js",
  "types": "index.d.ts",
  "contributors": [
    "Nathan Rajlich <nathan@tootallnate.net>",
    "Rod Vagg <r@va.gg>",
    "Juriy Zaytsev <kangax@gmail.com>"
  ],
  "keywords": [
    "canvas",
    "graphic",
    "graphics",
    "pixman",
    "cairo",
    "image",
    "images",
    "pdf"
  ],
  "homepage": "https://github.com/Automattic/node-canvas",
  "repository": "git://github.com/Automattic/node-canvas.git",
  "scripts": {
    "prebenchmark": "node-gyp build",
    "benchmark": "node benchmarks/run.js",
    "lint": "standard examples/*.js test/server.js test/public/*.js benchmarks/run.js lib/context2d.js util/*.js browser.js index.js",
    "test": "mocha test/*.test.js",
    "pretest-server": "node-gyp build",
    "test-server": "node test/server.js",
    "generate-wpt": "node ./test/wpt/generate.js",
    "test-wpt": "mocha test/wpt/generated/*.js",
    "install": "prebuild-install -r napi || node-gyp rebuild",
    "tsd": "tsd"
  },
  "files": [
    "binding.gyp",
    "browser.js",
    "index.d.ts",
    "index.js",
    "lib/",
    "src/",
    "util/"
  ],
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "prebuild-install": "^7.1.2",
    "simple-get": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "assert-rejects": "^1.0.0",
    "express": "^4.19.1",
    "js-yaml": "^4.1.0",
    "mocha": "^10.3.0",
    "pixelmatch": "^5.3.0",
    "standard": "^17.1.0",
    "tsd": "^0.30.7",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": "^18.12.0 || ^20.9.0 || >= 21"
  },
